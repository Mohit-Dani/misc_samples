<?xml version="1.0" encoding="UTF-8" ?>

<Configuration>
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%-5p] %C - %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root level="trace">
			<AppenderRef ref="STDOUT" level="INFO" />
		</Root>
		<Logger name="com.tomtom.places" level="INFO" >
			<AppenderRef ref="STDOUT" />
		</Logger>
		<Logger name="com.teleatlas.global" level="WARN" additivity="false">
			<AppenderRef ref="STDOUT" />
		</Logger>
		<Logger name="com.tomtom.cpu.coredb" level="WARN" additivity="false">
			<AppenderRef ref="STDOUT" />
		</Logger>
		<Logger name="org.apache" level="WARN" additivity="false">
			<AppenderRef ref="STDOUT" />
		</Logger>
	</Loggers>
</Configuration>
